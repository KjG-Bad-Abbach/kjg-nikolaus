{
  "kind": "collectionType",
  "collectionName": "time_slots",
  "info": {
    "singularName": "time-slot",
    "pluralName": "time-slots",
    "displayName": "Time Slot",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "start": {
      "type": "datetime",
      "required": true
    },
    "end": {
      "type": "datetime",
      "required": true
    },
    "max_bookings": {
      "type": "integer",
      "required": true,
      "default": 1,
      "min": 1
    },
    "bookings": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::booking.booking",
      "mappedBy": "time_slots"
    }
  }
}
